    <!-- Header -->
    <div class="header bg-gradient-info py-7 py-lg-8">
      <div class="container">
        <div class="header-body text-center mb-7">
          <div class="row justify-content-center">
            <div class="col-lg-5 col-md-6">
              <h1 class="text-white">Welcome to FinanceLogin</h1>
              <p class="text-lead text-light">Make Your Life Easy!</p>
            </div>
          </div>
        </div>
      </div>
      <div class="separator separator-bottom separator-skew zindex-100">
        <svg x="0" y="0" viewBox="0 0 2560 100" preserveAspectRatio="none" version="1.1" xmlns="http://www.w3.org/2000/svg">
          <polygon class="fill-default" points="2560 0 2560 100 0 100"></polygon>
        </svg>
      </div>
    </div>
    <!-- Page content -->
    <div class="container mt--8 pb-5">
      <!-- Table -->
      <div class="row justify-content-center">
        <div class="col-lg-6 col-md-8 layoutTop">
          <div class="card bg-secondary shadow border-0">
            <%# <div class="card-header bg-transparent pb-5">
              <div class="text-muted text-center mt-2 mb-4"><small>Sign up with</small></div>
              <div class="text-center">
                <a href="#" class="btn btn-neutral btn-icon mr-4">
                  <span class="btn-inner--icon"><img src="../assets/img/icons/common/github.svg"></span>
                  <span class="btn-inner--text">Github</span>
                </a>
                <a href="#" class="btn btn-neutral btn-icon">
                  <span class="btn-inner--icon"><img src="../assets/img/icons/common/google.svg"></span>
                  <span class="btn-inner--text">Google</span>
                </a>
              </div>
            </div> %>
            <div class="card-body px-lg-5 py-lg-5">
              <div class="text-center text-muted mb-4">
                <small>Register Now</small>
              </div>
              <%= simple_form_for (@account) do |account| %>
                <form role="form">
                  <%# <div class="form-group">
                    <div class="input-group input-group-alternative mb-3">
                      <div class="input-group-prepend">
                        <span class="input-group-text"><i class="ni ni-hat-3"></i></span>
                      </div>
                      <input class="form-control" placeholder="Org Name" type="text">
                    </div>
                  </div> %>
                  <div class="form-group">
                    <div class=" input-group-alternative mb-3">
                      <%= account.input :name, placeholder: "Name", class: "input-group input-group-prepend form-control", type: "text", label: false %>
                    </div>
                  </div>

                  <%= account.fields_for :owner do |owner| %>
                  <div class="form-group">
                    <div class="input-group-alternative mb-3">
                      <%= owner.input :email, class: "input-group input-group-prepend form-control", type: "email", placeholder: "Email Address", label: false %>
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="input-group-alternative">
                      <%= owner.input :password, class: "input-group input-group-prepend form-control", placeholder: "Password", type: "password", label: false %>
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="input-group-alternative">
                      <%= owner.input :password_confirmation, class: "input-group input-group-prepend form-control", placeholder: "Confirm Password", type: "password", label: false %>
                    </div>
                  </div>
                  <%# <div class="text-muted font-italic"><small>password strength: <span class="text-success font-weight-700">strong</span></small></div> %>
                  <%# <div class="row my-4">
                    <div class="col-12">
                      <div class="custom-control custom-control-alternative custom-checkbox">
                        <input class="custom-control-input" id="customCheckRegister" type="checkbox">
                        <label class="custom-control-label" for="customCheckRegister">
                          <span class="text-muted">I agree with the <a href="#!">Privacy Policy</a></span>
                        </label>
                      </div>
                    </div>
                  </div> %>
                  <% end %>
                  <div class="text-center">
                    <%= account.submit class: "btn btn-primary mt-4" %>
                    <%# <button type="button" class="btn btn-primary mt-4">Create account</button> %>
                  </div>
                </form>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>